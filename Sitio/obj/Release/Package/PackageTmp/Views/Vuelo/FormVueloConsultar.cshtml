@model IEnumerable<EC.Pasaje>

@{
    ViewBag.Title = "FormVueloConsultar";
}

<h2>Pasajes Vendidos</h2>

@ViewBag.Mensaje

<div>
    @Html.ActionLink("Volver al listado", "FormVuelosListar")
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cliente.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Asiento)
        </th>
        <th></th>
    </tr>
 @foreach (EC.Pasaje item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asiento)
            </td>
        </tr>
    }
</table>

